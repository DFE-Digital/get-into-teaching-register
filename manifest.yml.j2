---
applications:
- name: {{APPLICATION}}
  memory: 256M
  docker:
    image: {{DOCKER_IMAGE}}
  env:
    HTTPAUTH_PASSWORD: {{HTTPAUTH_PASSWORD}}
    HTTPAUTH_USERNAME: {{HTTPAUTH_USERNAME}}
    SECRET_KEY_BASE:   {{SECRET_KEY_BASE}}
    RAILS_ENV:         {{RAILS_ENV}}
    RAILS_MASTER_KEY:  {{RAILS_MASTER_KEY}}
