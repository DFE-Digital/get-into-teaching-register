<%= f.govuk_fieldset legend: { text: 'What is your address?' } do %>
  <%= f.govuk_email_field :address_line_1, width: 20,
    label: { text: 'Address line 1 *' } %>
  <%= f.govuk_text_field :address_line_2, width: 20,
    label: { text: 'Address line 2 *' } %>
  <%= f.govuk_text_field :town_city, width: 20,
    label: { text: 'Town or City *' } %>
  <%= f.govuk_text_field :postcode, width: 20,
    label: { text: 'Postcode *' } %>
  <%= f.govuk_submit %>
<% end %>