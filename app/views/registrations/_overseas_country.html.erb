<% countries = ApiClient::get_country_types.sort_by { |k| k.value } %>
<% countries.delete_if { |k| k.value == "United Kingdom" } %>

<%= f.govuk_collection_select :country_id,
  countries,
  :id,
  :value,
  label: { text: "Which country do you live in?", tag: "h1", size: "m" }
%>
<%= f.govuk_submit %>